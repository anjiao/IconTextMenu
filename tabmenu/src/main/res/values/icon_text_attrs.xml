<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="IconWithText">
        <attr name="menu_icon" format="reference" />
        <attr name="menu_icon_height" format="dimension" />
        <attr name="menu_icon_width" format="dimension" />
        <attr name="menu_icon_scaleType">
            <enum name="center" value="0" />
            <enum name="centerCrop" value="1" />
            <enum name="centerInside" value="2" />
            <enum name="fitCenter" value="3" />
            <enum name="fitStart" value="4" />
            <enum name="fitXY" value="5" />
        </attr>
        <attr name="menu_text_textSize" format="dimension" />
        <attr name="menu_text_textColor" format="color" />
        <attr name="menu_text_height" format="dimension" />
        <attr name="menu_text_width" format="dimension" />
        <attr name="menu_title" format="string" />
    </declare-styleable>

</resources>